<div class="container-fluid" id="archives">
  <h1>Completed Tasks</h1>
  <% @lists.each do |list| %>
    <% if list.archive %>
    <div class="row">
      <i class="fas fa-circle"></i>
      <div class="col">
        <h4><%= list.name %></h4>
      </div>
      <div class="w-100"></div>
      <% list.tasks.each do |task| %>
        <% if task.complete? %>
        <div class="row">
          <div class="col">
            <ul class="archives-tasks">
              <h5><i class="fa fa-check" aria-hidden="true"></i><%= task.name %></h5>
            </ul>
          </div>
        </div>
        <% end %>
      <% end %>
    </div>
    <% end %>
  <% end %>
</div>
